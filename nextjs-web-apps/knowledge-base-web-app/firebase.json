{
	"hosting": [
		{
			"cleanUrls": true,
			"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
			"public": "out",
			"rewrites": [
				{
					"source": "/posts/**/content",
					"destination": "/posts/[slug]/content.html"
				}
			],
			"site": "app-wallot-learn-production",
			"target": "live"
		},
		{
			"cleanUrls": true,
			"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
			"public": "out",
			"rewrites": [
				{
					"source": "/posts/**/content",
					"destination": "/posts/[slug]/content.html"
				}
			],
			"site": "app-wallot-learn-staging",
			"target": "test"
		}
	]
}
