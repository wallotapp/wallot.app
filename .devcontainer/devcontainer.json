{
	"build": {
		"dockerfile": "../Dockerfile",
		"context": ".."
	},
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.shell.linux": "/bin/bash"
			}
		}
	},
	"forwardPorts": [17101, 7101, 17102, 7102, 17150, 7150, 17159, 7159, 17157, 7157, 17152, 7152, 17153, 7153, 17154, 7154, 17158, 7158, 17151, 7151, 9005],
	"name": "Wallot Dev Container",
	"postStartCommand": "chmod +x ./scripts/on-start-container.sh && bash ./scripts/on-start-container.sh",
	"remoteEnv": {
		"DEVCONTAINER": "true"
	}
}
